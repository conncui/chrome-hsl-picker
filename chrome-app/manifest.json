{
    "manifest_version": 2,
    "name": "Color Picker and Converter",
    "short_name": "Color Picker and Converter",
    "description": "A simple HSLa/RGBa/HEX color picker and converter.",
    "version": "0.2.0",
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "offline_enabled": true,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "icons": {
        "16": "hsl-color-picker-16.png",
        "128": "hsl-color-picker-128.png"
    }
}
